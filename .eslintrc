{
  "parser"  : "babel-eslint",
  "globals": {
    angular: false,
    inject: false,
    module: false
  },
  "extends" : [
    "standard",
    "angular"
  ],
  "plugins": [
    "angular"
  ],
  "env"     : {
    "browser" : true,
    "mocha": true
  },
  "rules": {
    "semi" : [2, "never"],
    "max-len": [2, 120, 2]
  }
}
